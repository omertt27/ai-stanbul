# Production Environment Variables for Frontend
# Backend API URL (your production backend domain - update this with your Render URL)
VITE_API_URL=https://your-backend-app-name.onrender.com

# Environment
VITE_ENVIRONMENT=production

# Features
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false

# App Configuration
VITE_APP_NAME=AIstanbul
VITE_PORT=3000

# Security
VITE_SECURE_HEADERS=false

# Performance
VITE_ENABLE_COMPRESSION=false
