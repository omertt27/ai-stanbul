# Production Environment Variables for Frontend
# Copy to .env.production and fill in your actual values

# Backend API URL (your production backend domain)
VITE_API_URL=https://ai-stanbul.onrender.com

# Environment
VITE_ENVIRONMENT=production

# Features
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false

# App Configuration
VITE_APP_NAME=AIstanbul
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-powered travel guide for Istanbul

# Optional: Analytics
# VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
# VITE_HOTJAR_ID=your_hotjar_id_here
